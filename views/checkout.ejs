<script>
function applyOffer(){
    let promcode=document.getElementById('offcode').value
    let price= document.getElementById('grandtotal').innerHTML
    console.log(price);
    $.ajax({
        url:'/applyoffer',
        data:{
code:promcode,
price
        },
        method:'post',
        success:(resolve)=>{
console.log('success')
if(resolve.total,resolve.total){
    document.getElementById('totaldiscount').style.display='block';
    document.getElementById('totaldiscount').innerHTML=resolve.discount
document.getElementById('totalprice').innerHTML=resolve.total
document.getElementById('ttd').style.display='block';
document.getElementById('discount').innerHTML=resolve.discount
document.getElementById('Message').style.display='none';
}else{
    document.getElementById('Message').style.display='block';
    document.getElementById('totaldiscount').style.display='none';
    document.getElementById('ttd').style.display='none';
    document.getElementById('Message').innerHTML=resolve.Message
}


        }
    })
}

