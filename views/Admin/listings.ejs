<main>
<div class="container-fluid">
<h1 class="mt-4">Users</h1>
<ol class="breadcrumb mb-4">
<li class="breadcrumb-item"><a href="index.html">Dashboard</a></li>
<li class="breadcrumb-item active">Users</li>
</ol>
<div class="row">
<div class="col-xl-12">
<div class="card mb-4">
<div class="card-header">
<i class="fas fa-table mr-1"></i>
All Users
</div>
<div class="card-body">
<div class="table-responsive">
<table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
<thead>
<tr class="text-uppercase">
<th>User</th>
<th>User Name</th>
<th>Email</th>
<th>Mobile</th>
<th>State</th>
<th>Address</th>
<th>Action</th>
</tr>
</thead>
<tbody>

<tr>
    <%for (let i=0; i < users.length; i++){ %>
        <a href="">
<td><img class="img-profile rounded-circle" src="/img/user/1.png"></td>
<td> <%=users[i].username%>	</td>
<td><%=users[i].email%></td>
<td><%=users[i].mnumber%></td>
<td><%=users[i].status%></td>
<%if(users[i].address){%>
<td><%=users[i].address%></td>

<%}else{%>
    <td>Not Updated by the user</td>
    <%}%>
<%if(users[i].status=="unblocked"){%>
<td><a href="/admin/userBlock?id=<%=users[i]._id%>	" class="btn btn-primary btn-sm">Block</a><a href="/admin/orders?id=<%=users[i]._id%>" class="btn btn-primary btn-sm ml-4">View Orders</a></td>

<%}else{%>
    <td><a href="/admin/userUnblock?id=<%=users[i]._id%>" class="btn btn-primary btn-sm">Unblock</a><a href="/admin/orders?id=<%=users[i]._id%>" class="btn btn-primary btn-sm ml-4">View Orders</a></td>
    <%}%>
</a>
</tr>
<%}%>
</tbody>
</table>
</div>
</div>
</div>
</div>
