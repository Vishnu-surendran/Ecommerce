<h1 class="mt-4">Add listing</h1>
<ol class="breadcrumb mb-4">
<li class="breadcrumb-item"><a href="index.html">Dashboard</a></li>
<li class="breadcrumb-item active">Add Restaurant</li>
<%var Message%> 
</ol>
<div class="row">
<div class="col-xl-12">
<div class="card mb-4">
<div class="card-header">
Basic info
</div>
<label for="" style="text-align:center ;"><%=Message%></label>
<div class="card-body">
<div class="row">
<div class="col-md-6">
    <form class="" action="addproducts" method="post">
        <div class="form-group">
          <label for="exampleFormControlInput1">Product Name</label>
          <input type="text" class="form-control" id="exampleFormControlInput1" placeholder="Product Name" name="pname">
        </div>
        <div class="form-group">
          <label for="exampleFormControlSelect1">Category</label>
          <select class="form-control" id="exampleFormControlSelect1" name="pcategory">
            <%for (var i=0;i < resolve.name.length ; i++){%>
              <option><%=resolve.name[i]%></option>
              <%}%>
    
          </select>
        </div>
        <div class="form-group">
          <label for="exampleFormControlInput1">Price</label>
          <input type="number" class="form-control" id="exampleFormControlInput1" placeholder="Price" name="pname">
        </div>
        <div class="form-group">
          <label for="exampleFormControlSelect2">Location</label>
          <select class="form-control" id="exampleFormControlSelect1" name="plocation">
            <option>India</option>
            <option>America</option>
            <option>Newzealand</option>
          </select>
        </div>
        <div class="form-group">
          <label for="exampleFormControlSelect2">Restaurant Name</label>
          <select class="form-control" id="exampleFormControlSelect1" name="prestaurant">
      <%for (var i=0;i < restaurants.length ; i++){%>
            <option><%=restaurants[i].restaurantname%></option>
          <%}%>
      
          </select>
        </div>
        <div class="form-group">
          <label for="exampleFormControlTextarea1">Description</label>
          <textarea class="form-control" id="exampleFormControlTextarea1" rows="3" name="pdescription"></textarea>
        </div>
        <div class="mb-3">
          <label for="formFileMultiple" class="form-label">Select Images</label>
          <input class="form-control" type="file" id="formFileMultiple" name="rimage" multiple>
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
      </form>
</div>
